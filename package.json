        "cross-env": "^5.2.0",
        "jquery": "^3.2",
        "laravel-mix": "^2.1.14",
        "lodash": "^4.17.4",
        "popper.js": "^1.14.4",
        "tailwindcss": "^0.7.2",
        "vue": "^2.5.17"
    },
    "dependencies": {
        "glhd-tailwindcss-transitions": "^0.3.0",
        "glob-all": "^3.1.0",
        "laravel-mix-purgecss": "^2.2.3",
        "purgecss-webpack-plugin": "^1.2.1",
        "tailwindcss-tables": "^0.2.0"
    }
